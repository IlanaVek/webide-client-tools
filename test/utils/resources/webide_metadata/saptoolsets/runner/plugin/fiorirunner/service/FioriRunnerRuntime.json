{
	"name": "sap.watt.saptoolsets.fiori.run.fiorirunner.FioriRunnerRuntime",
	"methods": {
		"run": {
			"description": "Run an application using the Fiori Runner",
			"params": [{
				"name": "oWindow",
				"type": "object",
				"description": "the window instance"
			}, {
				"name": "oCustomData",
				"type": "object",
				"description": "run custom data"
			}, {
				"name": "oConfiguration",
				"type": "object",
				"description": "the application configuration"
			}, {
				"name": "oRunnableDocument",
				"type": "object",
				"description": "the application runnable document"
			}],
			"visibility": "private"
		},
		"getApplicationUrl": {
			"description": "Get URL for previewing the application using the Fiori Runner",
			"params": [{
				"name": "oConfiguration",
				"type": "object",
				"description": "the application configuration"
			}, {
				"name": "oRunnableDocument",
				"type": "object",
				"description": "the application runnable document"
			}, {
				"name": "oWindow",
				"type": "object",
				"description": "the window instance"
			}, {
				"name": "oCustomData",
				"type": "object",
				"description": "run custom data"
			}],
			"returns": {
				"type": "object",
				"description": "Application URL"
			},
			"visibility": "private"
		}
	},

	"configurationProperties": {
		"fiorirunneradapterrt": {
			"type": {
				"service": "sap.watt.saptoolsets.runner.fiorirunner.service.FioriRunnerAdapterRT"
			},
			"multiple": false
		}
	}
}