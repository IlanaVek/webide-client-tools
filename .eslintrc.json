{
  "extends": ["airbnb-base", "prettier"],
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "no-else-return": 0,
    "linebreak-style": 0,
    "arrow-body-style": 0,
    "strict": 0,
    "no-console": 0,
    "func-names": 0,
    "import/no-extraneous-dependencies": [
      2,
      {
        "devDependencies": true
      }
    ],
    "prefer-destructuring": 0,
    "no-buffer-constructor": 0
  },
  "globals": {}
}
